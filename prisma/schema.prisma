generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Provider {
  @@map("providers")
  id           String  @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  display_name String?
  public_id    String? @db.VarChar(20)
}
